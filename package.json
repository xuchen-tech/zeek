{
	"name": "zeek",
	"displayName": "zeek",
	"description": "zeek",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:zeekLogs"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "zeek.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "zeekLogs.refreshEntity",
				"title": "Refresh",
				"icon": {
					"dark": "resources/dark/refresh.svg",
					"light": "resources/light/refresh.svg"
				}
			},
			{
				"command": "fileExplorer.openFile",
				"title": "Open File"
			},
			{
				"command": "zeekContols.startZeek",
				"title": "Start Zeek"
			},
			{
				"command": "zeekContols.stopZeek",
				"title": "Stop Zeek"
			},
			{
				"command": "zeekContols.restartZeek",
				"title": "Restart Zeek"
			}
		],
		"configuration": [
			{
				"title": "Zeek Configuration",
				"properties": {
					"zeekctl.executable": {
						"type": "string",
						"default": "zeekctl",
						"description": "zeekctl executable path"
					},
					"zeek.logs": {
						"type": "string",
						"default": "/opt/zeek/logs/current",
						"description": "zeek logs path"
					},
					"jq.executable": {
						"type": "string",
						"default": "jq",
						"description": "jq executable path"
					},
					"zeekctl.cmd_options": {
						"type": [
							"array"
						],
						"default": [],
						"description": "the multiple "
					}
				}
			}
		],
		"views": {
			"explorer": [
				{
					"id": "zeekLogs",
					"name": "Zeek Logs"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "zeekLogs.refreshEntity",
					"when": "view == zeekLogs",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mkdirp": "^1.0.2",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/rimraf": "^3.0.2",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	}
}
